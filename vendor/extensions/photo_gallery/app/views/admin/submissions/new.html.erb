<h1>New Gallery Image</h1>

<%= error_messages_for :submission %>

<% form_for(:submission, :url => collection_url, :html => { :multipart => true }) do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>
  <p>
    <%= submit_tag t("create") %> or <%= link_to t("cancel"), collection_url %>
  </p>
<% end %>